{
  "name": "vue-webpack-project",
  "version": "0.0.1",
  "description": "A Webpack project, build a web page easily using vue.js",
  "author": "tiandongdong <tiandongdong@360.cn>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --open --progress  --config webpack/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "eslint . --ext .js,.vue --quiet && webpack --progress --hide-modules --config webpack/webpack.prod.conf.js",
    "lint": "eslint . --ext .js,.vue"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@q/webpack4-qcdn-file": "^1.0.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "precss": "^4.0.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
